alpha=0.05
t=t_stat(pop_mean=32.28,samp_mean=31.67,se=sample_error(std=1.29,n=19))
p=2*(1-st.t.cdf(df=18,x=np.absolute(t)))
if p<alpha:
    print("Null Hypothesis is rejected(ðœ‡!=$32.28)")
else:
    print("Null Hypothesis is accepted(ðœ‡=$32.28)")


