#try except
def fivebyzero(x,y):
    try:
        return x/y
    except ZeroDivisionError:
        return 0

fivebyzero(5,0)
    
#nested for loop

def sub():
    subjects=["Americans ","Indians"]
    verbs=["play","watch"]
    objects=["Baseball","Cricket"]

    for i in subjects:
        for j in verbs:
            for k in objects:
                print(i+" "+j+" "+k)
sub()
